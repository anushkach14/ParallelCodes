#include<stdio.h>
#include<stdlib.h>
#include<omp.h>

int main(int argc,char *argv[]){

	int numbers[20];
	int i;
	
	printf("Enter 20 intergers: \n");
	for( i=0;i<20;i++){
		scanf("%d",&numbers[i]);
	}
	
	#pragma omp for
	for( i=0;i<20;i++){
		if(numbers[i] % 2 == 0){
			printf("Even: %d\n",numbers[i]);
		}
		else{
			printf("Odd:  %d\n",numbers[i]);
		}
	}
	
	return 0;

}
